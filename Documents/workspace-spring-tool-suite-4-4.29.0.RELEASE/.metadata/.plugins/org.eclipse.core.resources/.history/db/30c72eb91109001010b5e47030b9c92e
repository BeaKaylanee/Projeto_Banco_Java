package exercicioslista5;

import java.util.HashMap;
import java.util.HashSet;
import java.util.Iterator;
import java.util.Map;
import java.util.Scanner;

public class CollectionssSet {

	public static void main(String[] args) {

		Scanner lerScanner = new Scanner(System.in);
		HashSet<Integer> setNumeros = new HashSet<Integer>();

		setNumeros.add(2);
		setNumeros.add(5);
		setNumeros.add(1);
		setNumeros.add(3);
		setNumeros.add(4);
		setNumeros.add(9);
		setNumeros.add(7);
		setNumeros.add(8);
		setNumeros.add(10);
		setNumeros.add(6);

		System.out.println("\nDados da Collection: " + setNumeros);

		System.out.println("\n\nListar todos os Elementos com o Iterator");

		Iterator<Integer> isetNumeros = setNumeros.iterator();

		while (isetNumeros.hasNext()) {
			System.out.println(isetNumeros.next());
		}

		System.out.print("Digite o número que você deseja encontrar: ");
		Integer numero = lerScanner.nextInt();
		Object numeroPesquisa = new Object();
		
		Map<Integer, Object> listaConta = new HashMap<>();
		listaConta.put(numero, numeroPesquisa);
		
		Object contaEncontrada = listaConta.get(12345);
		if (contaEncontrada != null) {
			System.out.println("O número"+ numero + " foi encontrado!\n");
		} else {
			System.out.println("O número "+ numero + " não foi encontrado");
		}
		
		//System.out.println("\nExiste a fruta Kiwi? " + setNumeros.contains(1));

	}
}
